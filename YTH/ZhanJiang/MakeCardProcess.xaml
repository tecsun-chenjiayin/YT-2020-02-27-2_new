                </Border>
                <Border Width="225">
                    <TextBlock HorizontalAlignment="Center" Name="m3" Text="2019.09.23"/>
                </Border>
                <Border Width="225">
                    <TextBlock HorizontalAlignment="Center" Name="m4" Text="2019.09.23"/>
                </Border>
                <Border Width="225">
                    <TextBlock HorizontalAlignment="Center" Name="m5" Text="2019.09.23"/>
                </Border>
                <Border Width="225">
                    <TextBlock HorizontalAlignment="Center" Name="m6" Text="2019.09.23"/>
                </Border>
                <Border Width="225">
                    <TextBlock HorizontalAlignment="Center" Name="m7" Text="2019.09.23"/>
                </Border>
            </StackPanel>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" TextBlock.FontSize="35" TextBlock.Foreground="White">
                <Border Width="225"/>
                <Image Name="p1" Source="../Soruce/Inages_ZQ/蓝色.png" Stretch="None"></Image>
                <Image Name="L1" Source="../Soruce/Inages_ZQ/横蓝.png" Stretch="None" Width="150" Margin="20,0,20,0"></Image>
                <Image Name="p2" Source="../Soruce/Inages_ZQ/蓝色.png" Stretch="None"></Image>
                <Image Name="L2" Source="../Soruce/Inages_ZQ/横灰.png" Stretch="None" Width="150" Margin="20,0,20,0"></Image>
                <Image Name="p3" Source="../Soruce/Inages_ZQ/灰色.png" Stretch="None"></Image>
                <Image Name="L3" Source="../Soruce/Inages_ZQ/横灰.png" Stretch="None" Width="150" Margin="20,0,20,0"></Image>
                <Image Name="p4" Source="../Soruce/Inages_ZQ/灰色.png" Stretch="None"></Image>
                <Image Name="L4" Source="../Soruce/Inages_ZQ/横灰.png" Stretch="None" Width="150" Margin="20,0,20,0"></Image>
