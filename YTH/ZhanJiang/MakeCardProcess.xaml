                <Border Width="225">
                    <TextBlock HorizontalAlignment="Center" Margin="20,0,20,0" Name="T3" TextWrapping="Wrap" Text="您的社保卡已完成卡片制作"/>
                </Border>
                <Border Width="225">
                    <TextBlock HorizontalAlignment="Center" Margin="20,0,20,0" Name="T4" TextWrapping="Wrap" Text="您的社保卡已寄出到市卡管理机构"/>
                </Border>
                <Border Width="225">
                    <StackPanel Orientation="Vertical">
                        <TextBlock HorizontalAlignment="Center" Margin="20,0,20,0" Name="T5" TextWrapping="Wrap" Text="您的社保卡已到达申领网点"/>
                        <TextBlock HorizontalAlignment="Center" Foreground="#FF1294FF" Margin="20,0,20,0" Name="T52" TextWrapping="Wrap" Text="（广州银行肇庆支行）"/>
                    </StackPanel>
                </Border>
                <Border Width="225">
                    <TextBlock HorizontalAlignment="Center" Margin="20,0,20,0" Name="T6" TextWrapping="Wrap" Text=""/>
                </Border>
                <Border Width="225">
                    <TextBlock HorizontalAlignment="Center" Margin="20,0,20,0" Name="T7" TextWrapping="Wrap" Text=""/>
                </Border>
            </StackPanel>
            <TextBlock Name="error" FontSize="25" Text="制卡失败：XXXXXXXXXX" TextWrapping="Wrap" HorizontalAlignment="Center" Margin="20" Foreground="Red"/>
            <StackPanel Orientation="Vertical" HorizontalAlignment="Center">
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" TextBlock.FontSize="50" Margin="0,10,0,0">
                    <TextBlock xml:space="preserve">姓　　名:  </TextBlock>
                    <TextBlock Name="name">刘先化</TextBlock>
                </StackPanel>
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" TextBlock.FontSize="50" Margin="0,10,0,0">
                    <TextBlock xml:space="preserve">身份证号:  </TextBlock>
                    <TextBlock Name="persionid">4417844564894</TextBlock>
                </StackPanel>
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" TextBlock.FontSize="50" Margin="0,10,0,0">
                    <TextBlock xml:space="preserve">制卡状态:  </TextBlock>
                    <TextBlock Name="status" Foreground="#FF1294FF">未申请</TextBlock>
                </StackPanel>
            </StackPanel>
