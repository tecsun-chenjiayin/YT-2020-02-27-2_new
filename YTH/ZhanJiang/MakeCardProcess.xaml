             d:DesignHeight="704" d:DesignWidth="1770">
    <Grid>
        <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
            <StackPanel HorizontalAlignment="Center" Orientation="Horizontal" TextBlock.FontSize="30" Margin="0,20,0,25">
