                <Image Name="p7" Source="../Soruce/Inages_ZQ/灰色.png" Stretch="None"></Image>
            </StackPanel>
            <StackPanel HorizontalAlignment="Center" Orientation="Horizontal" TextBlock.FontWeight="Bold" TextBlock.FontSize="40" Margin="0,25,0,0">
                <Border Width="225">
                    <TextBlock HorizontalAlignment="Center" FontSize="40" FontWeight="Bold" Text="业务环节"/>
                </Border>
                <Border Width="225">
                    <TextBlock HorizontalAlignment="Center" Foreground="#FF1294FF" Name="t1" Text="未申请"/>
                </Border>
                <Border Width="225">
                    <TextBlock HorizontalAlignment="Center" Name="t2" Text="已申请"/>
